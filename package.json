{
  "name": "nestjs-blog",
  "version": "1.1.0",
  "description": "blog developed by nestjs",
  "main": "index.js",
  "scripts": {
    "build:admin": "cd admin && npm run build && cross-env BUILD_TYPE=admin node ../build",
    "build:client": "cd client && npm run build && cross-env BUILD_TYPE=client node ../build",
    "build:server": "cd server && npm run build && cross-env BUILD_TYPE=server node ../build",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onfuns/nestjs-blog.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/onfuns/nestjs-blog/issues"
  },
  "homepage": "https://github.com/onfuns/nestjs-blog#readme",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.1.7",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4"
  }
}
