    "dayjs": "^1.11.7",
    "highlight.js": "^11.8.0",
    "lodash": "^4.17.21",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.7",
    "mobx": "^6.9.0",
    "mobx-react": "^7.6.0",
    "next": "^13.4.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
